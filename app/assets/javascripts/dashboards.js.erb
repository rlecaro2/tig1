 $(function() {
    new Highcharts.Chart({
    	
    	chart: {
        renderTo: "ingresos_chart"
		},
		title: {
		text: "Ingresos / Costos"
		},
		xAxis: {
		type: "datetime"
		},
		yAxis: {
		title: {
		  text: "CLP $"
		}
		},
		series: [
			{
				name: 'Ingresos',
				pointInterval: <%= 1.day * 1000%>,
				pointStart: <%= 2.weeks.ago.to_i * 1000%>,
				data: <%= (2.weeks.ago.to_date..Date.yesterday).map {|date| Reporte.ingresos_on(date).to_f }.inspect%>
			},
			{
				name: 'Costos',
				pointInterval: <%= 1.day * 1000%>,
				pointStart: <%= 2.weeks.ago.to_i * 1000%>,
				data: <%= (2.weeks.ago.to_date..Date.yesterday).map {|date| Reporte.costos_on(date).to_f }.inspect%>
			}
		]

    });
  });

