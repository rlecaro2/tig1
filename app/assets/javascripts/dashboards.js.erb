 $(function() {
    new Highcharts.Chart({
    	
    	chart: {
        renderTo: "ingresos_chart"
		},
		title: {
		text: "Ingresos / Costos"
		},
		xAxis: {
		type: "datetime"
		},
		yAxis: {
		title: {
		  text: "CLP $"
		}
		},
		series: [{
			pointsInterval: <%=1.day %>,
			pointStart: <%= 3.weeks.ago.to_i * 1000 %>,
			data: <%= (3.weeks.ago.to_date..Date.today).map {|date| Reporte.ingresos_on(date).to_f }%>
		}]

    });
  });

